drop table aluno;
use argentina;
create table aluno(
	matricula int NOT NULL,
    primary key(matricula),
    nome varchar (100) NOT NULL,
    data_nascimento date NOT NULL,
    cpf varchar (14) unique
);
desc aluno;

select * from aluno;
